@import "./style/iconfont.wxss";

page,
view,
text,
swiper,
swiper-item,
image,
navigator {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

page {
    /* 定义主题颜色 */
    --themeColor: #eb4450;
    /* 
    定义统一的字体大小，大小为375px 
    1px = 2rpx
    14px = 28rpx
    */
    font-size: 28rpx;
}

image {
    width: 100%;
}